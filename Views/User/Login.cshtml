@model Group5.ViewModels.LoginDto
@{
    Layout = null;
}
<!doctype html>
<html class="no-js" lang="">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Login Register | Notika - Notika Admin Template</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- favicon
        ============================================ -->
    <link rel="shortcut icon" type="~/nokita/image/x-icon" href="img/favicon.ico">
    <!-- Google Fonts
        ============================================ -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/bootstrap.min.css">
    <!-- font awesome CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/font-awesome.min.css">
    <!-- owl.carousel CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/owl.carousel.css">
    <link rel="stylesheet" href="~/nokita/css/owl.theme.css">
    <link rel="stylesheet" href="~/nokita/css/owl.transitions.css">
    <!-- animate CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/animate.css">
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/normalize.css">
    <!-- mCustomScrollbar CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/scrollbar/jquery.mCustomScrollbar.min.css">
    <!-- wave CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/wave/waves.min.css">
    <!-- Notika icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/notika-custom-icon.css">
    <!-- main CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/main.css">
    <!-- style CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/style.css">
    <!-- responsive CSS
        ============================================ -->
    <link rel="stylesheet" href="~/nokita/css/responsive.css">
    <!-- modernizr JS
        ============================================ -->
    <script src="~/nokita/js/vendor/modernizr-2.8.3.min.js"></script>
</head>

<body>
    <div>

        @if (ViewData.ContainsKey("ChangePassSuccessed"))
        {
            <p class="alert alert-warning" role="alert">@ViewData["ChangePassSuccessed"]</p>
        }

        @if (ViewData.ContainsKey("Error"))
        {
            <p class="alert alert-warning" role="alert">@ViewData["Error"]</p>
        }
        @if (ViewData.ContainsKey("Blocked"))
        {
            <p class="alert alert-warning" role="alert">@ViewData["Blocked"]</p>
        }
        @if (ViewData.ContainsKey("success"))
        {
            <p class="alert alert-success" role="alert">@ViewData["success"]</p>
        }

        @if (ViewData.ContainsKey("ErrorPassword"))
        {
            <p class="alert alert-success" role="alert">@ViewData["ErrorPassword"]</p>
        }
    </div>
 
    <div class="login-content">
 
        <!-- Login -->
        <div class="nk-block toggled" id="l-login">
            <div class="nk-form">
                <form asp-action="Login" asp-controller="User" method="post">
                    <div class="input-group">
                        <span class="input-group-addon nk-ic-st-pro"><i class="notika-icon notika-support"></i></span>
                        <div class="nk-int-st">
                            <input type="text" asp-for="Email" class="form-control" id="floatingInput" placeholder="Email">
                           
                            <span asp-validation-for="Email" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="input-group mg-t-15">
                        <span class="input-group-addon nk-ic-st-pro"><i class="notika-icon notika-edit"></i></span>
                        <div class="nk-int-st">
                            <input asp-for="Password" type="password" class="form-control" id="floatingPassword" placeholder="Password">
                            
                            <span asp-validation-for="Password" class="text-danger"></span>
                        </div>
                    </div>

                    <div class="input-group mg-t-15">
                        <span class="input-group-addon nk-ic-st-pro"><i class="notika-icon notika-right-arrow right-arrow-ant"></i></span>
                        <div class="nk-int-st">
                            <input  type="submit" class="form-control" value="Login" >
                        </div>
                    </div>


                    <div class="fm-checkbox">
                        <label><input type="checkbox" class="i-checks"> <i></i> Keep me signed in</label>
                    </div>
                    <a href="#l-register" data-ma-action="nk-login-switch" data-ma-block="#l-register" class="btn btn-login btn-success btn-float"><i class="notika-icon notika-right-arrow right-arrow-ant"></i></a>
                </form>
            </div>

            <div class="nk-navigation nk-lg-ic">
                <a href="#" data-ma-action="nk-login-switch" data-ma-block="#l-register"><i class="notika-icon notika-plus-symbol"></i> <span>Register</span></a>
                <a href="#" data-ma-action="nk-login-switch" data-ma-block="#l-forget-password"><i>?</i> <span>Forgot Password</span></a>
            </div>
        </div>

        <!-- Register -->
        <div class="nk-block" id="l-register">
            <div class="nk-form">
                <div class="input-group">
                    <span class="input-group-addon nk-ic-st-pro"><i class="notika-icon notika-support"></i></span>
                    <div class="nk-int-st">
                        <input type="text" class="form-control" placeholder="Username">
                    </div>
                </div>

                <div class="input-group mg-t-15">
                    <span class="input-group-addon nk-ic-st-pro"><i class="notika-icon notika-mail"></i></span>
                    <div class="nk-int-st">
                        <input type="text" class="form-control" placeholder="Email Address">
                    </div>
                </div>

                <div class="input-group mg-t-15">
                    <span class="input-group-addon nk-ic-st-pro"><i class="notika-icon notika-edit"></i></span>
                    <div class="nk-int-st">
                        <input type="password" class="form-control" placeholder="Password">
                    </div>
                </div>

                <a href="#l-login" data-ma-action="nk-login-switch" data-ma-block="#l-login" class="btn btn-login btn-success btn-float"><i class="notika-icon notika-right-arrow"></i></a>
            </div>

            <div class="nk-navigation rg-ic-stl">
                <a href="#" data-ma-action="nk-login-switch" data-ma-block="#l-login"><i class="notika-icon notika-right-arrow"></i> <span>Sign in</span></a>
                <a href="" data-ma-action="nk-login-switch" data-ma-block="#l-forget-password"><i>?</i> <span>Forgot Password</span></a>
            </div>
        </div>

        <!-- Forgot Password -->
        <div class="nk-block" id="l-forget-password">
            <div class="nk-form">
                <p class="text-left">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nulla eu risus. Curabitur commodo lorem fringilla enim feugiat commodo sed ac lacus.</p>

                <div class="input-group">
                    <span class="input-group-addon nk-ic-st-pro"><i class="notika-icon notika-mail"></i></span>
                    <div class="nk-int-st">
                        <input type="text" class="form-control" placeholder="Email Address">
                    </div>
                </div>

                <a href="#l-login" data-ma-action="nk-login-switch" data-ma-block="#l-login" class="btn btn-login btn-success btn-float"><i class="notika-icon notika-right-arrow"></i></a>
            </div>

            <div class="nk-navigation nk-lg-ic rg-ic-stl">
                <a href="" data-ma-action="nk-login-switch" data-ma-block="#l-login"><i class="notika-icon notika-right-arrow"></i> <span>Sign in</span></a>
                <a href="" data-ma-action="nk-login-switch" data-ma-block="#l-register"><i class="notika-icon notika-plus-symbol"></i> <span>Register</span></a>
            </div>
        </div>
    </div>
    <!-- Login Register area End-->
    <!-- jquery
        ============================================ -->
    <script src="~/nokita/js/vendor/jquery-1.12.4.min.js"></script>
    <!-- bootstrap JS
        ============================================ -->
    <script src="~/nokita/js/bootstrap.min.js"></script>
    <!-- wow JS
        ============================================ -->
    <script src="~/nokita/js/wow.min.js"></script>
    <!-- price-slider JS
        ============================================ -->
    <script src="~/nokita/js/jquery-price-slider.js"></script>
    <!-- owl.carousel JS
        ============================================ -->
    <script src="~/nokita/js/owl.carousel.min.js"></script>
    <!-- scrollUp JS
        ============================================ -->
    <script src="~/nokita/js/jquery.scrollUp.min.js"></script>
    <!-- meanmenu JS
        ============================================ -->
    <script src="~/nokita/js/meanmenu/jquery.meanmenu.js"></script>
    <!-- counterup JS
        ============================================ -->
    <script src="~/nokita/js/counterup/jquery.counterup.min.js"></script>
    <script src="~/nokita/js/counterup/waypoints.min.js"></script>
    <script src="~/nokita/js/counterup/counterup-active.js"></script>
    <!-- mCustomScrollbar JS
        ============================================ -->
    <script src="~/nokita/js/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <!-- sparkline JS
        ============================================ -->
    <script src="~/nokita/js/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/nokita/js/sparkline/sparkline-active.js"></script>
    <!-- flot JS
        ============================================ -->
    <script src="~/nokita/js/flot/jquery.flot.js"></script>
    <script src="~/nokita/js/flot/jquery.flot.resize.js"></script>
    <script src="~/nokita/js/flot/flot-active.js"></script>
    <!-- knob JS
        ============================================ -->
    <script src="~/nokita/js/knob/jquery.knob.js"></script>
    <script src="~/nokita/js/knob/jquery.appear.js"></script>
    <script src="~/nokita/js/knob/knob-active.js"></script>
    <!--  Chat JS
        ============================================ -->
    <script src="~/nokita/js/chat/jquery.chat.js"></script>
    <!--  wave JS
        ============================================ -->
    <script src="~/nokita/js/wave/waves.min.js"></script>
    <script src="~/nokita/js/wave/wave-active.js"></script>
    <!-- icheck JS
        ============================================ -->
    <script src="~/nokita/js/icheck/icheck.min.js"></script>
    <script src="~/nokita/js/icheck/icheck-active.js"></script>
    <!--  todo JS
        ============================================ -->
    <script src="~/nokita/js/todo/jquery.todo.js"></script>
    <!-- Login JS
        ============================================ -->
    <script src="~/nokita/js/login/login-action.js"></script>
    <!-- plugins JS
        ============================================ -->
    <script src="~/nokita/js/plugins.js"></script>
    <!-- main JS
        ============================================ -->
    <script src="~/nokita/js/main.js"></script>
</body>

</html>



   
